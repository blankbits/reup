{
    "s3_bucket": "reup-scratch",
    "s3_key_input": "historical-data/time-series/2020-04-01/SPY/time-series.csv.gz",
    "s3_key_output": "features/second/2020-04-01/SPY/features.csv.gz",
    "time_windows": [
        15,
        60,
        300,
        900,
        3600
    ],
    "logging": {
        "version": 1,
        "formatters": {
            "simple": {
                "format": "%(asctime)s %(levelname)-8s %(name)s - %(message)s"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "INFO",
                "formatter": "simple",
                "stream": "ext://sys.stdout"
            }
        },
        "root": {
            "level": "INFO",
            "handlers": [
                "console"
            ]
        }
    }
}
