{
    "polygon_time_series": {
        "s3_bucket": "reup",
        "s3_key_quotes": "polygon/2020-01-02/SPY/quotes.csv.gz",
        "s3_key_trades": "polygon/2020-01-02/SPY/trades.csv.gz",
        "s3_key_output": "polygon_time_series.csv.gz",
        "logging": {
            "version": 1,
            "formatters": {
                "simple": {
                    "format": "%(asctime)s %(levelname)-8s %(name)s - %(message)s"
                }
            },
            "handlers": {
                "console": {
                    "class": "logging.StreamHandler",
                    "level": "DEBUG",
                    "formatter": "simple",
                    "stream": "ext://sys.stdout"
                }
            },
            "root": {
                "level": "DEBUG",
                "handlers": [
                    "console"
                ]
            }
        }
    }
}
